program P
const int size=10;
int a+, b;
int a[];
class Table {
	int pos[], neg[];

	{
		void putp ( int a, int idx){
			this.pos[idx] = a+;
		}

		void putn(int a, int idx) {
			this.neg[idx] = a;
		}

		int getp(int idx) {
			return pos[idx];
		}

		int getn(int idx) {
			return neg[idx];
		}
	}
}
int b[];
Table val;
{
	void main()
	int x,i;
	{
		//‐‐‐‐‐‐‐‐‐‐ Initialize val
		val=new Table;
		val.pos=new int[size];
		val.neg=new int[size];
		for(i=0;i<size; i++)
		{
			val.putp(0,i);
			val.putn(0,i);
		}
		//‐‐‐‐‐‐‐‐‐‐ Read values
		read(x);
		for(;x>0;)
		{
			if(0<=x&&x<size)
			{
				val.putp(val.getp(x)+1);
			}else
				if(-size<x &&x< 0)
				{
					val.putn(val.getn(-x)+1);
				}
			read(x);
		}
	}
}